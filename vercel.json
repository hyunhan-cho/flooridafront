{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://app.floorida.site/api/$1"
        },
        {
            "source": "/teams/(.*)",
            "destination": "https://app.floorida.site/teams/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "installCommand": "npm install"
}